module Problem5

fun mdc(n : int, m : int) -> int {
    if m % n == 0 {
        return n
    } elif n % m == 0 {
        return m
    } else {
    	if m > n {
            return mdc(n, m%n)
	} else {
	   return mdc(m, n%m)
	}
    }
}

fun main() {
    println("Digite dois inteiros:")
    let x : int = getInt()
    let y : int = getInt()

    printf("O MDC entre ", x, " e ", y, " Ã© ", mdc(x,y))
}
